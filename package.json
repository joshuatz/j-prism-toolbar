{
  "name": "j-prism-toolbar",
  "version": "1.1.0",
  "description": "Quick toolbar thrown together for PrismJS. Typical \"windows\" style toolbar, with minimize/expand, fullscreen, and copy-to-clipboard buttons.\r ##",
  "main": "jPrismToolbar.js",
  "scripts": {
    "test": "jest --collectCoverage",
    "build": "parcel build jPrismToolbar.js --target browser --no-source-maps --out-file jPrismToolbar.min.js --out-dir . --experimental-scope-hoisting",
    "minify": "npm run build"
  },
  "keywords": [],
  "author": "Joshua Tzucker",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.2.7",
    "parcel": "^1.12.4"
  },
  "dependencies": {},
  "jest": {
    "testEnvironmentOptions": {
      "runScripts": "dangerously",
      "resources": "usable"
    }
  }
}
