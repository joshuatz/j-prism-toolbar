{
  "name": "j-prism-toolbar",
  "version": "1.2.3",
  "description": "Simple toolbar for PrismJS. Typical 'windows' style toolbar, with minimize/expand, fullscreen, and copy-to-clipboard buttons.",
  "main": "dist/jPrismToolbar.min.js",
  "scripts": {
    "test": "jest --collectCoverage",
    "build": "parcel build jPrismToolbar.js --target browser --no-source-maps --out-file jPrismToolbar.min.js --out-dir ./dist --experimental-scope-hoisting",
    "minify": "npm run build"
  },
  "keywords": [
    "prismjs",
    "prism",
    "decoration"
  ],
  "author": {
    "name": "Joshua Tzucker",
    "url": "https://joshuatz.com/?utm_source=package"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuatz/j-prism-toolbar.git"
  },
  "homepage": "https://github.com/joshuatz/j-prism-toolbar",
  "bugs": {
    "url": "https://github.com/joshuatz/j-prism-toolbar/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {},
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "files": [
    "dist/jPrismToolbar.min.js"
  ],
  "jest": {
    "testEnvironmentOptions": {
      "runScripts": "dangerously",
      "resources": "usable"
    }
  }
}
